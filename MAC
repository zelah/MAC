#include<stdio.h>
#include<stdlib.h>
#include<time.h>
 
#define AD(x,y) x=x+y
#define DV(x,y) x=x/y
#define HW VOhw()
#define HV(x) x=x/2
#define MP(a,t,f) t##mp(&t##f,a)
#define MU(x,y) x=x*y
#define PS(l,t,v) l=t##NDps(v,l)
#define RR(a,t,s) a=t##rr(s)
#define SB(x,y) x=x-y
#define SQ(x) x=x*x
#define TW(x) x=x+x
 
#define PROFILE srand(time(NULL))
#define PRO(s) PRO2(s)
#define PRO0(s)
#define PRO1(s) {int r=rand()/(RAND_MAX/PRO3);if(r==0){printf(":%s\n",s);fflush(stdout);printf("%c",1/r);}}
#define PRO2(s) {time_t t=time(NULL);int r=rand()/(RAND_MAX/PRO3);fflush(stdout);printf(":%s%3d ",s,(char)(t%100));fflush(stdout);printf("%c",1/r);}
#define PRO3 77
 
#define ND(t) typedef struct next {t v;struct next*n;} t##ND;
 
#define F1(t,n) t (*n)(t)
#define L(t,n) t##ND *n;n=NULL
#define P(t,n) t *n
 
#define HW0 void VOhw();
#define HW1 void VOhw(){PRO("HW");}
#define INC0(t) t t##inc(t n);
#define INC1(t) t t##inc(t n){PRO("INC");return ++n;}
#define MP0(t) t *t##mp(F1(t,f),P(t,a));
#define MP1(t) t *t##mp(F1(t,f),P(t,a)){t i;for(i=a[0];i>0;--i){a[i]=(*f)(a[i]);}PRO("MP");return a;}
#define PS0(t) t##ND *t##NDps(t v,t##ND *l);
#define PS1(t) t##ND *t##NDps(t v,t##ND *l){L(t,r);r=(t##ND*)malloc(sizeof(t##ND));r->v=v;r->n=l;return r;}
#define RR0(t) t *t##rr(t s);
#define RR1(t) t *t##rr(t s){P(t,a);a=(t*)malloc((s+1)*sizeof(t));a[0]=s;PRO("RR");return a;}
 
typedef char CH;
typedef unsigned char UC;
typedef signed char SC;
typedef short SH;
typedef unsigned short US;
typedef signed short SS;
typedef int IN;
typedef unsigned int UI;
typedef signed int SI;
typedef long LO;
typedef unsigned long LU;
typedef signed long LS;
typedef long long LL;
typedef unsigned long long UL;
typedef signed long long SL;
typedef float FL;
typedef double DB;
typedef long double LD;
ND(UC)
 
UC UCad(UC x,UC y);
UC UCad(UC x,UC y){PRO("ADD");return x+y;}
UC UCrd(UC *l,UC f,UC (*c)(UC,UC));
UC UCrd(UC *l,UC f,UC (*c)(UC,UC)){UC p;l[l[0]]=(*c)(l[l[0]],f);for(p=l[0]-1;p>0;--p){l[p]=(*c)(l[p],l[p+1]);}PRO("RD");return l[1];}

HW0
 
MP0(UC)
 
INC0(UC)
 
RR0(UC)
 
PS0(UC)
 
PS1(UC)
 
RR1(UC)
 
INC1(UC)
 
MP1(UC)
 
HW1
 
int main()
{
  PROFILE;
  DB x=1.000001;
  UL i;
  HW;
  for(i=1;i<999999999;++i)
  {
    if(x>2){x/=2;}
    SQ(x);
  }
  printf("%f",x);
  P(UC,a);
  RR(a,UC,3);
  a[1]='H';
  a[2]='A';
  a[3]='L';
  MP(a,UC,inc);
  printf("%c%c%c",a[1],a[2],a[3]);
  L(UC,list);
  PS(list,UC,'Z');
  printf(" %c ",list->v);
  UC u;
  a[1]=1;
  a[2]=2;
  a[3]=3;
  u=UCrd(a,0,&UCad);
  printf(" %d ",u);
  return 0;
}
