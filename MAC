#include<stdio.h>
#include<stdlib.h>
#include<time.h>
 
#define AD(x,y) x=x+y
#define DV(x,y) x=x/y
#define H VOh()
#define HV(x) x=x/2
#define MP(a,t,f) t##mp(&t##f,a)
#define MU(x,y) x=x*y
#define RR(a,t,s) a=t##rr(s)
#define SB(x,y) x=x-y
#define SQ(x) x=x*x
#define TW(x) x=x+x
 
#define PROFILE srand(time(NULL))
#define PRO(s) PRO1(s)
#define PRO0(s)
#define PRO1(s) {time_t t=time(NULL);printf(":%s%d%d ",s,1/(rand()/(RAND_MAX/PRO2)),(UC)(t-(256*(t/256))));}
#define PRO2 100
 
#define CH char
#define UC unsigned char
#define SC signed char
#define SH short
#define US unsigned short
#define SS signed short
#define IN int
#define UI unsigned int
#define SI signed int
#define LO long
#define LU unsigned long
#define LS signed long
#define LL long long
#define UL unsigned long long
#define SL signed long long
#define FL float
#define DB double
#define LD long double
#define ND(t) typedef struct next {t v;struct next*n;} t##ND;
 
#define F1(t,n) t (*n)(t)
#define L(t,n) t##ND *n;n=NULL
#define P(t,n) t *n
 
#define H0 void VOh();
#define H1 void VOh(){PRO("");}
#define INC0(t) t t##inc(t n);
#define INC1(t) t t##inc(t n){PRO("INC");return ++n;}
#define MP0(t) t *t##mp(F1(t,f),P(t,a));
#define MP1(t) t *t##mp(F1(t,f),P(t,a)){t i;for(i=a[0];i>0;--i){a[i]=(*f)(a[i]);}PRO("MP");return a;}
#define RR0(t) t *t##rr(t s);
#define RR1(t) t *t##rr(t s){P(t,a);a=(t*)malloc((s+1)*sizeof(t));a[0]=s;PRO("RR");return a;}
 
ND(UC)
 
H0
 
MP0(UC)
 
INC0(UC)
 
RR0(UC)
 
RR1(UC)
 
INC1(UC)
 
MP1(UC)
 
H1
 
int main()
{
  PROFILE;
  DB x=1.1;
  UL i;
  H;
  for(i=1;i<999999999;++i)
  {
    SQ(x);
  }
  printf("%f",x);
  P(UC,s);
  RR(s,UC,3);
  s[1]='H';
  s[2]='A';
  s[3]='L';
  MP(s,UC,inc);
  printf("%c%c%c",s[1],s[2],s[3]);
  L(UC,list);
  list=(UCND*)malloc(sizeof(UCND));
  list->v='Z';
  printf(" %c ",list->v);
  return 0;
}
